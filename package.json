  "scripts": {
    "start": "node scripts/start.js",
    "server": "NODE_ENV=development && nodemon --config nodemon.json",
    "build": "node scripts/build.js",
    "mongo": "docker-compose -f compose.yml up -d",
    "test": "node scripts/test.js"
  },
  "dependencies": {
    "@babel/core": "7.5.5",
    "@svgr/webpack": "4.3.2",
